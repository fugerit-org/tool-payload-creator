<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="Console"/>
    </Root>
    <Logger name="org.apache.fop.apps.FOUserAgent" level="error" additivity="false">
    </Logger>
    <Logger name="org.apache.fop.apps.FopConfParser" level="error" additivity="false">
    </Logger>
  </Loggers>
</Configuration>